void mainImage( out vec4 fragColor, in vec2 fragCoord ) {
    vec2 uv = fragCoord / iResolution.xy;
    vec3 ro = vec3(0,1,4),
        rd = normalize(vec3((2.0 * uv - 1.0) * 
        vec2(iResolution.y / iResolution.x), -1.0)),
        color = vec3(0);
    float t = 0.0;
    
    if (t < 1e-3) {
        color = vec3(1);
    }
    
    //float id = map(); 
    fragColor = vec4(color, 1);
}
